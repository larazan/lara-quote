{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.3 || ^8.0",
        "algolia/scout-extended": "^2.0",
        "barryvdh/laravel-snappy": "^1.0",
        "doctrine/dbal": "^3.6",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2.7",
        "laravel-notification-channels/twitter": "^6.2",
        "laravel/framework": "^9.19",
        "laravel/jetstream": "2.16",
        "laravel/sanctum": "^3.0",
        "laravel/socialite": "^5.13",
        "laravel/tinker": "^2.7",
        "laravelcollective/html": "^6.4",
        "laravolt/avatar": "^5.0",
        "league/commonmark": "^2.4",
        "livewire/livewire": "^2.11",
        "maatwebsite/excel": "^3.1",
        "mews/purifier": "^3.4",
        "psr/simple-cache": "^2.0",
        "shetabit/visitor": "^4.2",
        "spatie/laravel-permission": "^5.10",
        "spatie/laravel-searchable": "^1.11",
        "spatie/laravel-sitemap": "^6.3",
        "usernotnull/tall-toasts": "^1.7",
        "yarri/link-finder": "^2.7"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "app/Helpers/General.php"
        ]
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
